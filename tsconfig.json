        "./Engine/Source/Editor/Modules/AnimEditor/AnimEditorView.ts",
        "./Engine/Source/Editor/Modules/HeroEditor/HeroEditorView.ts",
        "./Engine/Source/Editor/tsd.d.ts",
        "./Engine/Source/Editor/Utils/fsUtil.ts",
        "./Engine/Source/Editor/WaspEditor.ts",
        "./Engine/Source/main.ts",
        "./Engine/Source/Runtime/Cameras/Camera.ts",
        "./Engine/Source/Runtime/Cameras/PerspectiveCamera.ts",
        "./Engine/Source/Runtime/Core/Channels.ts",
        "./Engine/Source/Runtime/Core/EventDispatcher.ts",
        "./Engine/Source/Runtime/Core/Geometry.ts",
        "./Engine/Source/Runtime/Core/Object3D.ts",
        "./Engine/Source/Runtime/Extras/Geometries/BoxGeometry.ts",
