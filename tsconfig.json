{
    "version": "next",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "outDir": "./Engine/Build"
    },
    "filesGlob": [
        "./Engine/Source/**/*.ts",
        "./Engine/Typings/**/*.ts"
    ],
    "files": [
        "./Engine/Source/Editor/UIEditor/try.ts",
        "./Engine/Source/Runtime/Core/Channels.ts",
        "./Engine/Source/Runtime/Core/EventDispatcher.ts",
        "./Engine/Source/Runtime/Core/Object3d.ts",
        "./Engine/Source/Runtime/Math/Core/Channels.ts",
        "./Engine/Source/Runtime/Math/Core/Object3D.ts",
        "./Engine/Source/Runtime/Math/Euler.ts",
        "./Engine/Source/Runtime/Math/MathUtil.ts",
        "./Engine/Source/Runtime/Math/Matrix3.ts",
        "./Engine/Source/Runtime/Math/Matrix4.ts",
        "./Engine/Source/Runtime/Math/Quaternion.ts",
        "./Engine/Source/Runtime/Math/Vector2.ts",
        "./Engine/Source/Runtime/Math/Vector3.ts",
        "./Engine/Source/Runtime/Math/Vector4.ts",
        "./Engine/Source/Runtime/Renderers/WebGLRenderTargetCube.ts",
        "./Engine/Source/Runtime/Renderers/WebGLRenderer.ts",
        "./Engine/Source/Runtime/Renderers/WebGLRendererTarget.ts",
        "./Engine/Source/Wasp.ts",
        "./Engine/Source/main.ts"
    ]
}
